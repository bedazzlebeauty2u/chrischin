{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}{% comment %} Search Modal {% endcomment %}
{% if settings.header_search %}
<div id="search-ctl" class="theme-ctl">
  <div class="inner">
    <span class="overlay"></span>
    <div class="ctl-actions">
      <a class="close-ctl" href="javascript:;" data-ctl-close=""><i class="zmdi zmdi-close"></i></a>
    </div>
    <div class="container">
      <div class="wrapper">
        <h2 class="title2 h1">{% comment %}ly_i18n_replace_for_[ 'search.general.title' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.title' | t }}{% endcapture %}{% include 'ly-static-string' with '2260499' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</h2>
        <div class="widget">
          <form action="/search" method="get" class="search-form" role="search">
            <input type="hidden" name="type" value="product">
            
            <div class="group-input">      
              <input type="search" name="q" autocomplete="off" required="required" />
              <span class="highlight"></span>
              <span class="bar"></span>
              <label>{% comment %}ly_i18n_replace_for_[ 'search.general.placeholder' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'search.general.placeholder' | t }}{% endcapture %}{% include 'ly-static-string' with '2260500' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</label>
              <button type="submit"><i class="zmdi"></i></button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
{% endif %}
{% comment %} Cart Success Modal {% endcomment %}
<div id="cart-ctl" class="theme-ctl theme-ctl2">
  <div class="inner">
    <span class="overlay"></span>
    <div class="central">
      <div class="item">
        <div class="heading">{% comment %}ly_i18n_replace_for_[ 'general.modal.add_to_cart_message' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.add_to_cart_message' | t }}{% endcapture %}{% include 'ly-static-string' with '2260501' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</div>
        <div class="cols">
          <div class="img">
            <a class="product-link" href="#"><img class="product-img" src="{% comment %}ly_asset_replace_for_['No_Image.png' | asset_url]_begin{% endcomment %}{% assign ly_asset = 'No_Image.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  }}{% comment %}ly_asset_replace_end{% endcomment %}" alt="" /></a>
          </div>
          <div class="info">
            <div class="product-title"><a class="product-link" href="#"></a></div>
            <div class="product-price"></div>
          </div>
          <div class="detail">
            <div class="product-qty">{% comment %}ly_i18n_replace_for_[ 'general.modal.quantify' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.quantify' | t }}{% endcapture %}{% include 'ly-static-string' with '2260502' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <span></span></div>
            <div class="product-total">{% comment %}ly_i18n_replace_for_[ 'general.modal.total' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.total' | t }}{% endcapture %}{% include 'ly-static-string' with '2260503' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <span></span></div>
          </div>
        </div>
      </div>
      <div class="cart">
        <div class="product-subtotal">{% comment %}ly_i18n_replace_for_[ 'general.modal.subtotal' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.subtotal' | t }}{% endcapture %}{% include 'ly-static-string' with '2260504' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} <span></span></div>
        <div class="cart-text">{% comment %}ly_i18n_replace_for_[ 'general.modal.cart_text' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.cart_text' | t }}{% endcapture %}{% include 'ly-static-string' with '2260505' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %} </div>
        <div>
          <button type="button" class="btn" onclick="roar.closeThemeCtl2();">{% comment %}ly_i18n_replace_for_[ 'general.modal.continue_shopping' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.continue_shopping' | t }}{% endcapture %}{% include 'ly-static-string' with '2260506' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</button>
          <a class="btn btn-1 ripple" href="/cart">{% comment %}ly_i18n_replace_for_[ 'general.modal.go_to_cart' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'general.modal.go_to_cart' | t }}{% endcapture %}{% include 'ly-static-string' with '2260507' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a>
        </div>
      </div>
    </div>
  </div>
</div>
